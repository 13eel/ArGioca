{
  "name": "argioca",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "tooling/*"
  ],
  "scripts": {
    "build": "turbo build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo clean",
    "lint": "turbo lint",
    "dev": "turbo dev",
    "dev:site": "turbo dev --filter=./apps/site",
    "dev:admin": "turbo dev --filter=./apps/amdin"
  },
  "packageManager": "bun@1.0.33"
}
